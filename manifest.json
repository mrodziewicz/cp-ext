{
  "manifest_version": 3,
  "name": "Copy Predefined Strings",
  "version": "1.0",
  "description": "Click to copy predefined strings to clipboard.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["clipboardWrite"]
}
